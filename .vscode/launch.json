{
	"version": "0.2.0",
	"configurations": [
        {
            "name": "Debug tests",
            "type": "node",
            "protocol": "auto",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
            "internalConsoleOptions": "openOnSessionStart",
			"sourceMaps": true,
            "args": [
                "JASMINE_CONFIG_PATH=jasmine.json",
				"--filter=respects inline rollbacks"
            ],
            "showAsyncStacks": true,
            "preLaunchTask": "build"
        }
	]
}