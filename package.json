{
  "name": "dialogue-builder",
  "version": "0.2.0",
  "description": "Beautifuly readable static dialogue - great for simple bots or an onboarding flow",
  "main": "dialogue-builder.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=4.3"
  },
  "dependencies": {
    "claudia-bot-builder": "../claudia-bot-builder"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.40",
    "@types/node": "^7.0.4",
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-es2015-destructuring": "^6.22.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "codecov": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-plugin-node": "^3.0.5",
    "gulp": "^3.9.1",
    "jasmine": "^2.5.2",
    "jasmine-promises": "^0.4.1",
    "jasmine-ts": "0.0.3",
    "line-number": "^0.1.0",
    "nyc": "^10.0.0",
    "ts-node": "^2.1.0",
    "typescript": "^2.1.4"
  },
  "scripts": {
    "prepublish": "tsc && babel dialogue-builder.js -o dialogue-builder.js && eslint dialogue-builder.js",
    "pretest": "tsc && babel dialogue-builder.js -o dialogue-builder.js && eslint dialogue-builder.js ",
    "test": "nyc jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "test-ts": "set JASMINE_CONFIG_PATH=jasmine.json&& jasmine-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nbransby/dialogue-builder.git"
  },
  "keywords": [
    "bot",
    "bot-builder",
    "lambda",
    "serverless",
    "claudia.js"
  ],
  "author": "Nicholas Bransby-Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nbransby/dialogue-builder/issues"
  },
  "homepage": "https://github.com/nbransby/dialogue-builder#readme"
}
